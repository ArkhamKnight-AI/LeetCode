class Solution {
public:
    int moreThanHalfNum_Solution(vector<int>& nums) {
        int cnt = 1, x = nums[0];
        
        for(int i = 1; i < nums.size(); i++){
            if(nums[i] == x) cnt++;
            else{
                cnt--;
                x = cnt < 0 ? nums[i] : x;
                cnt = cnt < 0 ? 1 : cnt;
            }
        }
        
        return x;
    }
};
