class Solution {
public:

int getUglyNumber(int n) {
    int p2 = 1, p3 = 1, p5 = 1;

    vector<int> dp(n + 1);

    dp[1] = 1;

    for (int i = 2; i <= n; i++) {
        int num2 = 2 * dp[p2], num3 = 3 * dp[p3], num5 = 5 * dp[p5];
        dp[i] = min(num2, min(num3, num5));
        if (dp[i] == num2) p2++;

        if (dp[i] == num3) p3++;

        if (dp[i] == num5) p5++;
    }

    return dp[n];
}
};
