//使用单调队列
class Solution {
public:
	//Insert one char from stringstream
	queue<char> q;
	unordered_map<char, int> map;

	void insert(char ch) {
		map[ch]++;
		if (map[ch] > 1) {
			while (!q.empty() && map[q.front()] > 1) q.pop();
		}

		else {
			q.push(ch);
		}
	}
	//return the first appearence once char in current stringstream
	char firstAppearingOnce() {
		if (q.empty()) return '#';
		return q.front();
	}
};
